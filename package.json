{
  "name": "imessage-prototype",
  "version": "1.0.1",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "ios": "expo run:ios",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "generate-apple-token": "node scripts/generate-apple-music-token.js",
    "setup-hooks": "chmod +x scripts/setup-hooks.sh .githooks/pre-commit .githooks/pre-push && ./scripts/setup-hooks.sh",
    "sb": "EXPO_PUBLIC_STORYBOOK_ENABLED=true expo start",
    "sb:ios": "EXPO_PUBLIC_STORYBOOK_ENABLED=true expo run:ios",
    "sb:generate": "sb-rn-get-stories",
    "android": "expo run:android",
    "knip": "knip",
    "markdownlint": "markdownlint '**/*.md' --ignore node_modules --ignore CHANGELOG.md --ignore ios --ignore android",
    "markdownlint:fix": "markdownlint '**/*.md' --ignore node_modules --ignore CHANGELOG.md --ignore ios --ignore android --fix"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{js,jsx}": [
      "prettier --write"
    ],
    "*.{json,css}": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write",
      "markdownlint --ignore node_modules --ignore CHANGELOG.md"
    ]
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.57.0",
    "@expo/ui": "~0.2.0-beta.9",
    "@gorhom/bottom-sheet": "^5.1.8",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/native": "^7.1.16",
    "@react-navigation/native-stack": "^7.3.23",
    "@react-navigation/stack": "^7.4.4",
    "expo": "~54.0.33",
    "expo-audio": "~1.1.1",
    "expo-blur": "~15.0.8",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-image": "~3.0.11",
    "expo-linear-gradient": "~15.0.8",
    "expo-module-scripts": "^5.0.8",
    "expo-status-bar": "~3.0.9",
    "expo-symbols": "~1.0.8",
    "openai": "^5.11.0",
    "react": "19.1.0",
    "react-native": "^0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-worklets": "0.5.1",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.6",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@storybook/addon-actions": "^9.0.8",
    "@storybook/addon-ondevice-actions": "^9.1.0",
    "@storybook/addon-ondevice-controls": "^9.1.0",
    "@storybook/addon-ondevice-notes": "^9.1.0",
    "@storybook/react-native": "^9.1.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "^29.5.14",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "babel-loader": "^8.4.1",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^54.0.16",
    "jsonwebtoken": "^9.0.2",
    "knip": "^5.83.1",
    "lint-staged": "^16.2.7",
    "markdownlint-cli": "^0.47.0",
    "prettier": "^3.6.2",
    "react-dom": "19.1.0",
    "semantic-release": "^24.2.9",
    "storybook": "^9.1.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
